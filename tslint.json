{
  "defaultSeverity": "warning",
  "extends": "tslint-config-airbnb",
  "linterOptions": {
    "exclude": ["node_modules/**", "build/**", "e2e/**"]
  },
  "jsRules": {
    "curly": true
  },
  "rules": {
    "curly": true,
    "no-console": false,
    "member-access": false,
    "import-name": false,
    "object-shorthand-properties-first": false,
    "trailing-comma": false
  },
  "rulesDirectory": []
}
